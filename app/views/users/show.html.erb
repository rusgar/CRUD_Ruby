<%= @user.username %>

<%=turbo_frame_tag "products-#{@pagy.page}", class: "product" do %>
      <%= render partial: 'products/product', collection: @products %>
      <%= turbo_frame_tag "products-#{@pagy.next}", class: "product", src: pagy_url_for(@pagy, @pagy.next),
       loading: :lazy  if @pagy.next %>
<%end %> 
